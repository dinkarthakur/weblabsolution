<!DOCTYPE html>
<html>
<head>
  <title>Weblabsolution</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "vendor/custom.modernizr" %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>
<body>

<!-- Header -->
<div class="row">
  <div class="width40 large-3 columns">
    <h1 class="margin-top8px"><%= link_to image_tag("weblabsolution.jpg"),{:controller => "home", :action => "index"}%></h1>
  </div>
  <div class="width60 large-9 columns">
    <ul class="right button-group">
      <li><a href="#" class="button">Link 1</a></li>
      <li><a href="#" class="button">Link 2</a></li>
      <li><a href="#" class="button">Link 3</a></li>
      <li><a href="#" class="button">Link 4</a></li>
      <li><a href="#" class="button">Link 5</a></li>
      <li><a href="#" class="button">Link 6</a></li>
    </ul>
  </div>
</div>

<!-- End Header-->

<%= yield %>

<!-- Footer -->

<footer class="row">
  <div class="large-12 columns">
    <hr />
    <div class="row">
      <div class="large-6 columns">
        <p>&copy; Copyright no one at all. Go to town.</p>
      </div>
      <div class="large-6 columns">
        <ul class="inline-list right">
          <li><a href="#">Link 1</a></li>
          <li><a href="#">Link 2</a></li>
          <li><a href="#">Link 3</a></li>
          <li><a href="#">Link 4</a></li>
        </ul>
      </div>
    </div>
  </div>
</footer>

<!-- End Footer -->

</body>
</html>
